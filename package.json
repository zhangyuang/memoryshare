{
  "name": "memoryshare",
  "version": "1.0.4",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "memoryshare",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "i686-pc-windows-msvc"
      ]
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "README.md"
  ],
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.15.2",
    "@types/koa": "^2.15.0",
    "ava": "^5.1.1",
    "benny": "^3.7.1",
    "cross-env": "^7.0.3",
    "koa": "^2.14.2",
    "shelljs": "^0.8.5",
    "tsx": "^4.7.1"
  },
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "node scripts/build.js",
    "build:debug": "napi build --platform",
    "universal": "napi universal",
